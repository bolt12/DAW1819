extends layout

block corpo
    .w3-container
        .w3-card-4
            header.w3-container.light-grey
                h1 Indice Musical
            .w3-container
                table.w3-table.w3-all
                    tr
                        th Id
                        th Titulo
                    each p in ind
                        tr
                            td= p.id
                            td
                                a(href= "http://localhost:12345/json/"+p.id+".json")= p.titulo
            footer.w3-container.w3-light-grey
                address Gerado por PartiApp